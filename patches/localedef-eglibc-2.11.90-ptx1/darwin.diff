diff -Nurp localedef-eglibc-2.11.90-ptx1/import-eglibc.sh localedef-darwin/import-eglibc.sh
--- localedef-eglibc-2.11.90-ptx1/import-eglibc.sh	1970-01-01 01:00:00.000000000 +0100
+++ localedef-darwin/import-eglibc.sh	2012-02-12 16:29:03.000000000 +0100
@@ -0,0 +1,12 @@
+#!/bin/sh
+
+eglibc="${1}"
+top="$(dirname "${0}")"
+
+rm -rf "${top}/eglibc"
+cat "${top}/glibc-files" | while read file target; do
+	target="${top}/eglibc/${target:-${file}}"
+	mkdir -p "$(dirname "${target}")" &&
+	cp "${eglibc}/${file}" "${target}" || exit 1
+done
+
